pipeline{
agent any
{
stages{
stage(' compile stage')
{
steps {
withMaven(maven: 'maven-3.8.7'){
sh 'mvn clean compile'
}
}
}

stage ('Testing stage') {
steps{
withMaven(maven: 'maven-3.8.7'){
sh 'mvn test'
}
}
}

stage ('Deployment stage') {
steps{
withMaven(maven: 'maven-3.8.7'){
sh 'mvn deploy'
}
}
}

}
}
}
